# Plugin properties
plugin.htmlviewer.HtmlViewerPlugin.name=Info Viewer
plugin.htmlviewer.HtmlViewerPlugin.activate=startup

plugin.htmlviewer.HtmlViewerPlugin.author=Dirk Moebius, Alan Ezust
plugin.htmlviewer.HtmlViewerPlugin.version=1.6
plugin.htmlviewer.HtmlViewerPlugin.docs=index.html
plugin.htmlviewer.HtmlViewerPlugin.description=A Swing-based web browser, which can also provide a replacement for HelpViewer.
plugin.htmlviewer.HtmlViewerPlugin.depend.0=jedit 04.03.99.00
plugin.htmlviewer.HtmlViewerPlugin.depend.1=jdk 1.5
#plugin.htmlviewer.HtmlViewerPlugin.depend.2=plugin lucene.LucenePlugin 2.0

# Two option panes
plugin.htmlviewer.HtmlViewerPlugin.option-group=chooseBrowser internalBrowser

# first optionpane
options.chooseBrowser.label=Choose Browser
options.chooseBrowser.code=new htmlviewer.HtmlViewerOptionPane();

# second optionpane 
options.internalBrowser.label=Internal Browser
options.internalBrowser.code=new htmlviewer.HtmlViewerOptionPane2();

# plugin menu
plugin.htmlviewer.HtmlViewerPlugin.menu = htmlviewer helpviewer - htmlviewer.open-buffer htmlviewer.open-sel

# Plugin menu properties 
htmlviewer-toggle.label=Toggle HtmlViewer
helpviewer-toggle.label=Toggle HelpViewer
htmlviewer.menu.label=HtmlViewer
htmlviewer.label=HtmlViewer
helpviewer.label=HelpViewer
htmlviewer.open-buffer.label=Open current buffer with browser
htmlviewer.open-sel.label=Open selected URL with browser

# Default option pane properties
htmlviewer.browsertype=internal
htmlviewer.class=infoviewer.HelpHtmlViewer
htmlviewer.method=showInternalInfoViewer
htmlviewer.otherBrowser=netscape -remote openURL($u) -raise
htmlviewer.homepage=jeditresource:/HtmlViewer.jar!/index.html
htmlviewer.appearance.floating.showMenu=true
htmlviewer.appearance.floating.showToolbar=true
htmlviewer.appearance.floating.showAddressbar=true
htmlviewer.appearance.floating.showStatusbar=true
htmlviewer.appearance.docked.showMenu=false
htmlviewer.appearance.docked.showToolbar=true
htmlviewer.appearance.docked.showAddressbar=false
htmlviewer.appearance.docked.showStatusbar=true
htmlviewer.autoupdate=false
htmlviewer.autoupdate.onSwitch=false
htmlviewer.autoupdate.onSave=false
htmlviewer.autoupdate.onChange=false
htmlviewer.autoupdate.onChange.delay=3000
htmlviewer.autoupdate.periodically=false
htmlviewer.autoupdate.periodically.delay=20000


