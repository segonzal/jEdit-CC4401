# jEdit only needs to load the plugin the first time the user accesses it
# the presence of this property also tells jEdit the plugin is using the new API
plugin.umlviewer.UMLViewerPlugin.activate=defer

# Even if you don't store additional files, this is a good idea to set:
plugin.umlviewer.UMLViewerPlugin.usePluginHome=true

# Required for all plugins:
plugin.umlviewer.UMLViewerPlugin.name=UMLViewer
plugin.umlviewer.UMLViewerPlugin.author=Américo Jerez, Matías Rodrígez

# version number == jEdit version number
plugin.umlviewer.UMLViewerPlugin.version=0.1Alpha

# we only have one dependency, jEdit 5.2pre1
plugin.umlviewer.UMLViewerPlugin.depend.0=jdk 1.7
# See jEdit.getBuild() to understand version numbering scheme. 
plugin.umlviewer.UMLViewerPlugin.depend.1=jedit 05.02.00.00

plugin.umlviewer.UMLViewerPlugin.description=Versión 0.0alpha de UMLViewer, intentando hacer que aparezcan cosas.

plugin.umlviewer.UMLViewerPlugin.menu=umlviewer \
	- \
	umlviewer.generate-uml

umlviewer.label=UMLViewer

umlviewer.generate-uml.label=Generar UML

umlviewer.title=UMLViewer